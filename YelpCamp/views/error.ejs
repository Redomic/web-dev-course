<%- block('title').append('ERROR') %> <%- layout('layouts/boilerplate.ejs') %>
<div class="row">
  <div class="col-8 offset-2">
    <div class="alert alert-danger" role="alert">
      <h3 class="alert-heading text-center"><%= err.statusCode %></h3>
      <h4 class="text-center"><%= err.message %></h4>
      <hr />
      <p><%= err.stack %></p>
    </div>
  </div>
</div>
